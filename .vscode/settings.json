{
  "code-runner.executorMap": {
    "typescript": "cd $dir && npx tsx $fullFileName",
    "powershell": "pwsh -ExecutionPolicy ByPass -File",
    "python": "set PYTHONIOENCODING=utf-8 && set PYTHONPATH=. && $pythonPath $fullFileName"
  },
  "code-runner.executorMapByFileExtension": {
    ".ts": "cd $dir && npx tsx $fullFileName"
  },
  "codingcopilot.enableCloseCompletionShortcutTip": true,
  "codingcopilot.modelResponseLanguage": "中文",
  "codingcopilot.CommitMessageStyle": "ConventionalCommits",
  "codingcopilot.commitMessageLanguage": "en",
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "python.createEnvironment.contentButton": "show",
  "python.defaultInterpreterPath": "",
  "python.languageServer": "Pylance",
  "python.missingPackage.severity": "Warning",
  "python.REPL.enableREPLSmartSend": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.mcs": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    "**/__pycache__": true,
    "**/node_modules": true
  },
  "explorerExclude.backup": {},
  "explorer.excludeGitIgnore": false,
  "python.terminal.shellIntegration.enabled": true,
  "python.testing.pytestEnabled": true,
  "python.useEnvironmentsExtension": true,
  "python.testing.unittestEnabled": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.globalModuleInstallation": false,
  "python-envs.defaultPackageManager": "",
  "python-envs.pythonProjects": [],
  "terminal.integrated.env.osx": {
    "PYTHONPATH": "${workspaceFolder}/"
  },
  "terminal.integrated.env.linux": {
    "PYTHONPATH": "${workspaceFolder}/"
  },
  "terminal.integrated.env.windows": {
    "PYTHONPATH": "${workspaceFolder}/"
  },
  "editor.tokenColorCustomizations": {
    "comments": "",
    "textMateRules": []
  },
  "python.terminal.useEnvFile": true
}